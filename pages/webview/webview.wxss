/* 提示窗 */
.modal_wrap{
  position: fixed;
  top:0;
  left:50%;
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left:-25%;
}
.modal{
  font-size: 28rpx;
  color:#fff;
  padding:15rpx 20rpx;
  background-color: rgba(0,0,0,0.6);
}
.hidden{
   display: none; 
}