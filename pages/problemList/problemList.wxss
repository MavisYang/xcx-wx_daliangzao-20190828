/* problemList.wxss */
.pro_list_wrap{
  padding: 30rpx 0 0;
}
.each_pro{
  height:100rpx;
  font-size: 24rpx;
  font-weight: bold;
  line-height: 100rpx;
  position: relative;
  color: #242424;
  background-color: #fff;
  padding: 0 0 0 36rpx;
  border-bottom: 1px solid #e2e2e2;
}
.with_arrow{
  position: absolute;
  top: 50%;
  right: 30rpx;
  width: 16rpx;
  height: 31rpx;
  margin-top:-15.5rpx;
}
.pro_footer{
  width: 100%;
  height: 98rpx;
  font-size: 24rpx;
  line-height: 98rpx;
  text-align: center;
  color:#fff;
  background-color: #b4a078;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 2;
}